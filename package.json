{
  "name": "styled-state-spring",
  "version": "1.0.0",
  "description": "A React library focused on State / Animation / Layout / Styles",
  "author": "rkhayat",
  "license": "MIT",
  "repository": "rkhayat/styled-state-spring",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "storybook": "start-storybook",
    "build:storybook": "build-storybook -c .storybook -o ./docs/",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "prettier": "prettier --write src/**/*.js",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d docs"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "react-spring": ">= 8.0.11",
    "styled-components": ">= 4.1.0",
    "xstate": ">= 4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-stage-0": "^7.0.0",
    "@material-ui/core": "^3.9.3",
    "@storybook/addon-options": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@svgr/rollup": "^2.4.1",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.1.4",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "immutability-helper": "^3.0.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "^1.1.4",
    "react-spring": "^8.0.19",
    "react-test-renderer": "^16.8.6",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0",
    "storybook-readme": "^5.0.1",
    "styled-components": "^4.2.0",
    "xstate": "^4.4.0"
  },
  "files": [
    "dist"
  ]
}
